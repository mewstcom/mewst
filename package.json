    "@popperjs/core": "^2.11.7",
    "@rails/ujs": "^7.0.4",
    "autosize": "^6.0.1",
    "bootstrap": "^5.3.0-alpha3",
    "esbuild": "^0.17.18",
    "sass": "^1.64.1"
  },
  "devDependencies": {
    "prettier": "2.8.8"
  }
}
